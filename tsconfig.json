{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "types": ["webpack-env"],
    "baseUrl": ".",
    "paths": {
      "main_panel/*": ["src/*"]
    },
    "typeRoots": ["src", "node_modules/@types"]
  },
  "include": ["src"]
}
